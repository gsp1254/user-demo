<div style="text-align: center">
<h1>Users</h1>
</div>
<table>
<tr>
<td style="text-align: center"> Name </td>
<td style="text-align: center"> Email </td>
<td style="text-align: center"> Campaigns List </td>
</tr>
<% @users.each do |user|%>
<tr>
<td><%= user.name %></td>
<td><%= user.email %></td>
<td><%= user.campaigns_list %></td>
</tr>
<% end %>
</table>

